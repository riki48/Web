@using WebApp.Data.Models;
@model IEnumerable<Car>

@{ 
	Layout = null;
}

<!DOCTYPE html>

<html>
	<head>
		<meta name="viewport" content="width=device-width" />
		<title>List</title>
	</head>
    <body>
		<h2>Все автомобили</h2>
		@{ 
			foreach (var item in Model )
			{
				<div>
			         <h2>Модель:@item.Name </h2>
					 <p> Цена: @item.Price.ToString("c")</p>
				</div>

			}
		}
	</body>

</html>